{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    },
    "Path": {
      "Serilog": "/mnt/logs/promo-code.txt"
    }
  },
  "AllowedHosts": "*",
  "Secrets": {
    "HmacPubSubHub": "testSecret",
    "CallbackApiKey": "1234",
    "YoutubeApiKey": "your-youtube-api-key",
    "OpenAIApiKey": "[yourKey]"
  },
  "ConnectionStrings": {
    "Sqlite": "/mnt/PromoCodes.db"
  },
  "Hub": {
    "Callback": "https://promo-codes.duckdns.org/api/youtube-feed",
    "Topic": "https://www.youtube.com/xml/feeds/videos.xml"
  },
  "WorkerDelay": {
    "ProcessVideo": 1
  }
}
